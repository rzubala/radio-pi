{
  "expo": {
    "name": "Radio player",
    "slug": "radio-pi-player",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/radio.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.zubala.rafal.radio_pi",
      "versionCode": 1
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
